
<% @doctors.each do |doctor| %>
  <section class="user">
    <div class="image">
      <%= image_tag 'doctor.png' %>
    </div>
    <div class="summary">
      <h2>
        <%= link_to "#{doctor.first_name} #{doctor.last_name}", doctor %>
      </h2>
      <h4>
        <%= doctor.category.name %>
      </h4>
    </div>
  </section>
<% end %>
